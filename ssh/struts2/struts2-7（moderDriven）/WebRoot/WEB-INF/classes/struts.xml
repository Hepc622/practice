<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE struts PUBLIC
	"-//Apache Software Foundation//DTD Struts Configuration 2.3//EN"
	"http://struts.apache.org/dtds/struts-2.3.dtd">
<struts>
	<package name="employee" extends="struts-default">
		<action name="user-login" class="com.hniu.modeldriven.LoginModelDriven"
			method="login">
			<result name="login">show.jsp</result>
		</action>
		<action name="user-attr" class="com.hniu.attr.LoginAttr"
			method="attr">
			<result name="attr">show.jsp</result>
		</action>
	</package>
</struts>

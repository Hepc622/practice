<?xml version="1.0" encoding="UTF-8"?>
<!DOCTYPE hibernate-mapping PUBLIC 
    "-//Hibernate/Hibernate Mapping DTD 3.0//EN"
    "http://www.hibernate.org/dtd/hibernate-mapping-3.0.dtd">
<hibernate-mapping>
	<class name="com.hniu.surveypark.model.Answer" table="answers">
		<id column="answer_id" name="answer_id" type="java.lang.Integer">
			<generator class="identity"></generator>
		</id>

		<property name="question_id" column="question_id" type="java.lang.Integer" />

		<property name="survey_id" column="survey_id" type="java.lang.Integer" />

		<property name="question_answer" column="question_answer"
			type="java.lang.String" />

		<property name="question_other_answer" column="question_other_answer"
			type="java.lang.String" />

		<property name="uuId" column="uuId" type="java.lang.String" />

		<property name="createDate" column="createDate" type="java.util.Date" />
	</class>
</hibernate-mapping>
    